{
	"outputs":{
        "print_verbosity": 2
    },
    "data_source":{
		"device": "corsano-2872b",
		"sensor_type": "PPG",
		"file_paths": ["data/corsano2872b_ppg_green.csv"]
	},
    "filter": {
        "sample_rate": 32,
        "lowcut": 0.15,
        "highcut": 15,
        "order": 4
    },
    "ppg_preprocessing": {
        "threshold": 10000,
        "min_duration": 30
    },
    "ppg_processing": {
        "beat_detector": "ampd",
        "plot": true,
        "plot_save": false,
        "plot_save_path": "output/visuals",
        "sqi_group_size": 10,
        "sqi_type": "bpm_plausible"
    }
}
